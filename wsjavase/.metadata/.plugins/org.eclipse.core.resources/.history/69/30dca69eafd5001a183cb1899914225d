public class TestePessoa {

	public static void main(String[] args) {
		Aluno objeto = new Aluno();
		objeto.setEmail("dbrogergmail.com");
		System.out.println(objeto.verificarDados());
		
		Professor objeto2 = new Professor();
		objeto2.setValorHora(10);
		System.out.println(objeto2.verificarDados());
	}

}

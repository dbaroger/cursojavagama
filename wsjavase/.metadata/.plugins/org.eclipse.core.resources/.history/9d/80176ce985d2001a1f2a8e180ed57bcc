package br.com.universidade.teste;

import br.com.universidade.modelo.Aluno;  // ctrl+shift+O  importa automaticamente

public class TesteAluno {

	public static void main(String[] args) {
		
		Aluno objeto = new Aluno();
		objeto.preencherNome("Roger");
		objeto.preencherRm(987298318);
		objeto.preencherEmail("dbaroger@gmail.com");
		System.out.println(objeto.exibirNome());
		System.out.println(objeto.exibirEmail());
		System.out.println(objeto.exibirRm());
	}

}

package br.com.universidade.modelo;

public class Aluno {
	
	private String nome;
	private String email;
	private int rm;
	
	// <modificador> <retorno> <nomoDoMetodo> (<tipoParametro> <nomeParametro, ...>) {}
	public void preencherNome(String pNome) {
		nome = pNome;
	}
	
	public String exibirNome(){
		return nome;
	}
	
	public void preencherEmail(String pEmail) {
		email = pEmail;
	}
	
	public String exibirEmail(){
		return email;
	}
	
	public void preencherRm(int pRm) {
		rm = pRm;
	}
	
	public int exibirRm(){
		return rm;
	}
	
	public void preencherTudo(String pNome, String pEmail, int pRm) {
		nome = pNome;
		email = pEmail;
		rm = pRm;
	}
	
	public String exibirTudo(){
		return nome + email + rm;
	}
}

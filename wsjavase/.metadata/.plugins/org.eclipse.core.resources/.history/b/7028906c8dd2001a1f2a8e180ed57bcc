package br.com.universidade.teste;

import javax.swing.JOptionPane;

import br.com.universidade.modelo.Aluno;  // ctrl+shift+O  importa automaticamente

public class TesteAluno2 {

	public static void main(String[] args) {
		
		Aluno objeto = new Aluno();
		objeto.preencherTudo(JOptionPane.showInputDialog("Digite o nome"), 
							JOptionPane.showInputDialog("Digite o email", 
							Integer.parseInt(JOptionPane.showInputDialog("Digite o RM"));
		System.out.println(objeto.exibirTudo());
		
		//JOptionPane.showMessageDialog(null,  "Nome: " + objeto.exibirNome() + "\nEmail: " + objeto.exibirEmail() + "\nRM: " + objeto.exibirRm());
	}

}
